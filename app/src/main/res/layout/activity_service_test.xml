<?xml version="1.0" encoding="utf-8"?>
<layout>

    <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:gravity="center"
        android:orientation="vertical"
        tools:context=".ServiceTestActivity">

        <Button
            android:id="@+id/fetch_vendor_id"
            android:text="Fetch vendor id"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />

        <Button
            android:id="@+id/cloud_firmware_check"
            android:text="cloud firmware check"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />

        <Button
            android:text="Support_pdf_auto_decrypt"
            android:id="@+id/support_pdf_auto_decrypt"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />

        <Button
            android:text="Support_pdf_auto_decrypt"
            android:id="@+id/test"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />

    </LinearLayout>
</layout>
